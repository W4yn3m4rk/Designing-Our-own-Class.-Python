class Vehicle:
    def move(self):
        print("The vehicle moves")

class Car(Vehicle):
    def move(self):
        print("Driving on the road ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying in the sky âœˆï¸")

class Boat(Vehicle):
    def move(self):
        print("Sailing on the water ğŸš¤")

# Polymorphism in action
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    v.move()
